# k8s and docker
alias ku='kubectl'
alias dp='docker system prune'
alias dps='docker ps'
alias dims='docker images'
alias dup='docker compose up -d'
alias ddown='docker compose down'

# homebrew updates
alias brewski='brew update && brew upgrade && brew cleanup; brew doctor'

export WORK=$HOME/Work

# git shortcut
alias gs='git status'
alias gcom='git checkout master'
alias gcod='git checkout development'
alias gcos='git checkout staging'

# define path to project directory
alias repo-airflow='cd $WORK/data-airflow-dags/'
alias repo-data-tools='cd $WORK/data-tools/'
alias repo-collective='cd $WORK/data-collective-recommend/'
alias repo-k8s-chart='cd $WORK/data-k8s-chart/'

# k8s context switching
alias use-minikube='kubectl config use-context minikube'

# file
alias getfeed="python3 $WORK/data-tools/getfeedv4/get_feed_v4.py"
alias hello-hk01="python3 $WORK/data-tools/getlink/getlink.py"
alias getnotes="vim $WORK/.notes"
alias sqlworkbenchj='java -jar /Applications/SQLWorkbenchJ.app/Contents/Java/sqlworkbench.jar'
alias jl='jupyter-lab'

# check aws log
alias athena-log="aws athena get-query-execution --query-execution-id"

# ssh connection
alias ssh-db="ssh jumphost -N -L 5432:localhost:5432"

